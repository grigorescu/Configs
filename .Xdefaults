! Features
URxvt.scrollBar: off
URxvt*saveLines:	       	5500
URxvt*cursorBlink:              false
URxvt.cutchars:                 ()*,<>[]{}|

! Font settings
URxvt*font:			xft:Source Code Pro Medium:pixelsize=13:antialias=true:hinting=true
URxvt*boldFont:                 xft:Source Code Pro Semibold:pixelsize=13

! Color settings

URxvt.depth:			32

! https://github.com/chriskempson/tomorrow-theme/blob/master/Xdefaults/Tomorrow%20Night

URxvt*background:       	#1d1f21
URxvt*foreground:       	#c5c8c6
URxvt*cursorColor:		#aeafad

!black
URxvt*color0:		        #000000
URxvt*color8:			#666666

!red
URxvt*color1:	                #912226
URxvt*color9:			#cc6666

!green
URxvt*color2:	                #778900
URxvt*color10:			#b5bd68

!yellow
URxvt*color3:	                #ae7b00
URxvt*color11:			#f0c674

!blue
URxvt*color4:	                #1d2594
URxvt*color12:			#81a2be

!magenta
URxvt*color5:	                #682a9b
URxvt*color13:			#b294bb

!cyan
URxvt*color6:	                #2b6651
URxvt*color14:			#8abeb7

!white
URxvt*color7:	                #929593
URxvt*color15:                  #ecebec

